{
  "name": "@mrszlv/ai-ui-components",
  "version": "0.1.3",
  "description": "AI-UI React components with OpenAI/Groq provider + auto-fallback",
  "type": "module",
  "private": false,
  "license": "MIT",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "style": "dist/index.css",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./dist/index.css": "./dist/index.css",
    "./index.css": "./dist/index.css",
    "./style.css": "./dist/index.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "engines": {
    "node": ">=18.18"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "build:css": "npx tailwindcss -i src/tailwind.css -o dist/index.css --minify",
    "build:all": "npm run build && npm run build:css"
  }
}
